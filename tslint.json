{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "jsx-curly-spacing": true,
    "jsx-no-bind": true,
    "jsx-no-multiline-js": true,
    "jsx-self-close": true,
    "quotemark": [true, "single", "jsx-double"],
    "ordered-imports": false,
    "semicolon": false,
    "no-console": false,
    "no-unused-variable": true,
    "arrow-parens": false,
    "member-access": false,
    "no-shadowed-variable": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "interface-over-type-literal": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}